{
  "name": "cam-pro-mcp-server",
  "version": "0.2.0",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "[service]-mcp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/mcp_server/src/*.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/mcp_server/src/index.js",
    "start:stdio": "node dist/index.js --stdio",
    "dev": "tsc && node dist/index.js",
    "dev:stdio": "tsc && node dist/index.js --stdio"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "dotenv": "^17.2.1",
    "form-data": "^4.0.4",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "shx": "^0.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  }
}
